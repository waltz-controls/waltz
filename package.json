{
  "name": "waltz",
  "version": "1.0.6",
  "description": "Waltz controls - lightweight SCADA web frontend",
  "main": "index.html",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf codebase",
    "test": "mocha-webpack --require test/setup.js --webpack-config webpack.test.config.js \"test/**/*.test.js\"",
    "lint": "eslint ./",
    "build": "webpack --env.production true",
    "module": "webpack --env.production true --env.module true",
    "standalone": "webpack --env.production true --env.module true --env.standalone true",
    "start": "webpack-dev-server --env",
    "user-context-server": "node test.server.js",
    "war": "rm -rf dist && node war.js $npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waltz-controls/waltz.git"
  },
  "keywords": [
    "waltz",
    "waltz-controls",
    "tango-controls",
    "web",
    "jive",
    "astor",
    "epics",
    "tine"
  ],
  "author": "Ingvord",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/waltz-controls/waltz/issues"
  },
  "homepage": "https://www.waltz-controls.space",
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.4",
    "archiver": "^3.1.1",
    "atob": "^2.1.2",
    "babel-loader": "^8.1.0",
    "btoa": "^1.2.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.7.2",
    "express": "^4.17.1",
    "file-loader": "^0.11.2",
    "fs": "0.0.1-security",
    "jsdom": "^11.6.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.0.3",
    "mocha-webpack": "2.0.0-beta.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^4.0.1",
    "script-loader": "^0.7.2",
    "style-loader": "^1.2.1",
    "url-loader": "^0.5.9",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@waltz-controls/eventbus": "^1.2.4",
    "@waltz-controls/middleware": "^2.0.0",
    "@waltz-controls/tango-rest-client": "^1.5.1",
    "@waltz-controls/waltz-grid-widget": "^1.0.0",
    "@waltz-controls/waltz-tango-rest-plugin": "^2.0.2",
    "@waltz-controls/waltz-user-actions-plugin": "^2.0.3",
    "@waltz-controls/waltz-user-context-plugin": "^2.0.0",
    "@waltz-controls/waltz-webix-extensions": "^1.0.14",
    "codemirror": "^5.54.0",
    "jquery": "^3.5.1",
    "plotly.js-dist": "^1.54.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rxjs": "^6.5.5",
    "webix": "^7.3.0"
  }
}
